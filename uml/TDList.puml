@startuml
'https://plantuml.com/class-diagram

class TDManagerController {

}

class TDViewer {
    + displayTODOs(TDList)
    + filteredDisplayTODOs(group: TDGroup, done: boolean)
    + filteredDisplayTODOs(list: TDList, done: boolean)
}

class Exporter {
    + exportToJSON(list: TDList);
}

File Output {

}

File Input {

}

class Importer {
    + loadFromJSON(filepath: String): TDList
}

class TDManager {
    tdGroup: TDGroup

    + getGroups(): TDGroup or ArrayList<TDGroup>
}

class TDList {
    title: String
    list: ArrayList<Item>

    + rename(newTitle: String)
    + addItem(item: Item)
    + rmItem(index: int)
}

class Item {
    description: String
    done: boolean
    dueDate: Date(YYYY-MM-DD)

    + editDescription(newDesc: String)
    + editDueDate(newDate: Date(YYYY-MM-DD))
    + setDone(done: boolean)
}

javafx.Application <|-- TDViewer
TDManager <-- TDManagerController
TDList o-- Item
TDManager <-- TDList
TDViewer <-- TDManager
TDManager --> Exporter
TDManager <-- Importer
Output <-- Exporter
Input --> Importer

@enduml